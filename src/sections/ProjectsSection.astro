    img: "/projects/nosomostv.webp",
    github: "https://github.com/malsebasmal/NoSomosTv-StaticPage",
    urlPage: "https://nosomostv.netlify.app"
  },
  {
    name: "Gallery Music",
    description: "Una galería musical de mis géneros favoritos más escuchados.",
    img: "/projects/gallerymusic.webp",
    github: "https://github.com/malsebasmal/GalleryMusic-StaticPage",
    urlPage: "https://gallerymusic.netlify.app"
  },
  {
